{
  "name": "cypress-svelte-unit-test",
  "description": "Unit testing Svelte components in Cypress E2E test runner",
  "version": "3.3.2",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": "https://github.com/bahmutov/cypress-svelte-unit-test/issues",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist"
  ],
  "types": "dist",
  "homepage": "https://github.com/bahmutov/cypress-svelte-unit-test#readme",
  "keywords": [
    "cypress",
    "svelte",
    "test"
  ],
  "license": "MIT",
  "main": "dist",
  "private": false,
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/cypress-svelte-unit-test.git"
  },
  "scripts": {
    "ban": "ban",
    "deps": "deps-ok && dependency-check --no-dev .",
    "issues": "git-issues",
    "license": "license-checker --production --onlyunknown --csv",
    "lint": "prettier --write 'cypress/**/*.js' 'lib/*.ts'",
    "pretest": "npm run lint",
    "size": "npm pack --dry",
    "test": "npm run cypress:run",
    "unused-deps": "dependency-check --unused --no-dev .",
    "cypress:open": "cypress open-ct",
    "cypress:run": "cypress run-ct",
    "build": "tsc",
    "watch": "tsc -w",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@cypress/webpack-dev-server": "^1.4.0",
    "html-webpack-plugin": "4.5.2",
    "unfetch": "4.1.0",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.5",
    "axios": "0.18.0",
    "ban-sensitive-files": "1.9.2",
    "cypress": "^7.7.0",
    "dependency-check": "3.3.0",
    "dependency-version-badge": "1.10.0",
    "deps-ok": "1.2.2",
    "git-issues": "1.3.1",
    "license-checker": "16.0.0",
    "mocha": "5.2.0",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "rxjs": "6.5.1",
    "sass": "1.26.5",
    "semantic-action": "1.1.6",
    "semantic-release": "17.4.4",
    "svelte": "3.29.4",
    "svelte-loader": "2.13.6",
    "svelte-preprocess": "4.7.3",
    "typescript": "3.9.9",
    "webpack": "4.41.5"
  },
  "standard": {
    "globals": [
      "cy",
      "Cypress"
    ]
  }
}
